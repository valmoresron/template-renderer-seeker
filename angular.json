{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "template-renderer": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "type": "component"
        },
        "@schematics/angular:directive": {
          "type": "directive",
          "skipTests": true
        },
        "@schematics/angular:service": {
          "type": "service",
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "typeSeparator": ".",
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "typeSeparator": ".",
          "skipTests": true
        },
        "@schematics/angular:module": {
          "typeSeparator": "."
        },
        "@schematics/angular:pipe": {
          "typeSeparator": ".",
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "typeSeparator": ".",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "baseHref": "./",
            "browser": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/fonts.css",
              {
                "glob": "**/*",
                "input": "template-data"
              },
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.scss"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/fonts.css",
                {
                  "glob": "**/*",
                  "input": "template-data"
                }
              ],
              "outputHashing": "none",
              "optimization": true,
              "sourceMap": false,
              "extractLicenses": true,
              "namedChunks": false,
              "aot": true,
              "serviceWorker": false
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "options": {
            "port": 4300
          },
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "template-renderer:build:production"
            },
            "development": {
              "buildTarget": "template-renderer:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        }
      }
    }
  },
  "cli": {
    "analytics": "346f9216-1342-4e11-842d-10fe2890ce3c"
  }
}